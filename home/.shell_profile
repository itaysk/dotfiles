# common for bash and zsh

LC_CTYPE=en_US.UTF-8
LC_ALL=en_US.UTF-8
export EDITOR=vim

# aliases
alias ll="ls -laGH"
alias tmux="tmux -u"

export PATH="/usr/local/sbin:$PATH"

#asdf
. $(brew --prefix asdf)/asdf.sh

#z
. $(brew --prefix z)/etc/profile.d/z.sh

